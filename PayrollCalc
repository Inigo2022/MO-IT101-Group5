    public PayrollCalc(String n, String s, String a, String e, String b, String pn, String sn, String ph, String ad, String ti, String pi, String st, String po, String is, double hw, double hpr, double dw, double dpr, double sss)
    {
        this.name = n;
        this.surname = s;
        this.address = a;
        this.employeeid= e;
        this.birthday = b;
        this.phonenumber = pn;
        this.sssno = sn;
        this.philhealthno = ph;
        this.tinno = ti;
        this.pagibigno = pi;
        this.status = st;
        this.position = po;
        this.immediatesupervisor = is;
        this.hoursWorked = hw;
        this.hourlyPayRate = hpr;
        this.dailyhoursworked = dw;
        this.dailypayrate = dpr; 
        this.sss = sss;
        
    }    
        
        
     
 public double grossPay(double hourlyRate, double hrsWorked)
 {
        double grossPay = (hourlyRate * hrsWorked);
        return grossPay;
 }
 
 public double grosspay(double dailyworked, double dailyhourworked)
 {
    	double grossPay = (dailypayrate * dailyhoursworked );
    	return grossPay;
    	
 }
      
 public double philhealth(double grosspay)
 {
		return philhealth;
  		
      
 } 	
        int philhealth;
        double grosspay;
	    
			
		{
	    if (grosspay >= 0.00 && grosspay <= 10000) {
	        philhealth = 300;
	       } else if (grosspay >= 10000 && grosspay <= 60000) {
	        philhealth = 600;
          }
        }
			
		
  public double pagibig(double grosspay)
{
		return pagibig;
			  		
} 	
						
		int pagibig;
						
		{
		if (grosspay >= 0.00 && grosspay <= 10000) {
		    pagibig = 50;
		   } else if (grosspay >= 10000 && grosspay <= 60000) {
		    pagibig = 100;
	      }
	    }	

 public double ricesubsidyallowance(double grosspay)
{
		return ricesubsidyallowance;
									  											      
} 	
												
		int ricesubsidyallowance;												
		{
	    if (grosspay >= 0.00 && grosspay <= 22000) {
		    ricesubsidyallowance = 1500;
		    } else if (grosspay >= 23000 && grosspay <= 80000) {
			ricesubsidyallowance = 3000;
			} else {
				 
	      }
        }	
					
 public double phoneallowance(double grosspay)
{
		return phoneallowance;
											  		
} 	
		double phoneallowance;
													
{
		if (grosspay >= 0.00 && grosspay <= 22000) {
		    phoneallowance = 500;
		    } else if (grosspay >= 23000 && grosspay <= 40000) { 
			phoneallowance = 800;
					 
			} else if (grosspay >= 23000 && grosspay <= 80000) {
			phoneallowance = 1000;
			} else {
			
	       }						
	     }	
			
 public double clothingallowance(double grosspay)
{
			return clothingallowance;										  													      
} 	
														
		 double clothingallowance;											
		 {
		 if (grosspay >= 0.00 && grosspay <= 22000) {
			clothingallowance = 500;
			} else if (grosspay >= 23000 && grosspay <= 40000) { 
			clothingallowance = 800;
			} else if (grosspay >= 23000 && grosspay <= 80000) {
		     clothingallowance= 1000;
			} else {
		    
	        }						
		  }		
		 
		 
						

public double tax(double grosspay){
   return tax;
}
 public double tax;
{
	 {
		
			} if (grosspay <= 20832){
				tax = 0;
                                }
                            else if(grosspay >= 20832 && grosspay < 33333) {
				tax = 0.20 ;
		    	}
                            else if(grosspay >= 33333 && grosspay < 66667){ 
				tax = 0.25 ;
		    	 }
                            else if(grosspay >= 66667 && grosspay < 166667){ 
				tax = 0.30 ;
		    	 }
                            else if(grosspay >= 166667 && grosspay < 666667){ 
				tax = 0.32 ;
		    	 }
                            else if(grosspay > 666667 ){ 
				tax = 0.35 ;		
                         }
			
			
    
	 
         }



    
